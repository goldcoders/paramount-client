{
	"name": "Flutter CI",
	"image": "cirrusci/flutter:3.7.7",
    "forwardPorts": [3000],
	"features": {
		"ghcr.io/devcontainers/features/sshd:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"Dart-Code.dart-code",
				"Dart-Code.flutter",
				"luanpotter.dart-import",
				"gmlewis-vscode.flutter-stylizer",
				"jeroen-meijer.pubspec-assist",
				"hbenl.vscode-test-explorer",
				"Gruntfuggly.todo-tree",
				"blaxou.freezed",
				"circlecodesolution.ccs-flutter-color",
				"KiritchoukC.flutter-clean-architecture",
				"FelixAngelov.bloc",
				"usernamehw.errorlens",
				"donjayamanne.githistory",
				"eamodio.gitlens",
				"mhutchie.git-graph",
				"humao.rest-client",
				"Luxcium.pop-n-lock-theme-vscode"
			]
		}
	}
}
